    "@willbooster/prettier-config": "10.0.0",
    "@willbooster/wb": "8.0.42",
    "build-ts": "13.2.1",
    "chalk": "5.4.1",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "lefthook": "1.11.12",
    "prettier": "3.5.3",
    "semantic-release": "24.2.3",
    "sort-package-json": "3.0.0",
    "typescript": "5.8.3",
    "undici": "7.8.0",
